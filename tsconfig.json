{
	"compilerOptions": {
		"target": "es2020",
		"lib": ["esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"esModuleInterop": true,
		"module": "CommonJS",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": false,
		"declaration": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"emitDecoratorMetadata": true,
		"rootDir": "./src",
		"plugins": [],
		"paths": {
			// "@/*": ["./*"]
		},
		"outDir": "indexer/lib"
	},
	// "include": ["common/**/*.ts", "src/**/*.ts"],
	"include": ["src/**/*"]
	// "exclude": ["node_modules", "lib"]
}
