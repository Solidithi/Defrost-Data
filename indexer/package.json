{
  "name": "defrost-indexer",
  "version": "1.0.0",
  "main": "lib/main.js",
  "scripts": {
    "build": "rm -rf lib/ && tsc",
    "start": "node lib/main.js",
    "abi:gen": "npx squid-evm-typegen src/typegen-abi ./abi/*",
    "typeorm:gen": "squid-typeorm-codegen && squid-typeorm-migration generate",
    "typeorm:migrate": "squid-typeorm-migration apply",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@subsquid/evm-abi": "^0.3.1",
    "@subsquid/evm-codec": "^0.3.0",
    "@subsquid/evm-processor": "^1.27.1",
    "@subsquid/graphql-server": "^4.9.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "dotenv": "^16.4.7",
    "ethers": "v6",
    "pino": "^9.6.0",
    "pino-roll": "^3.1.0",
    "priority-queue-typescript": "^2.0.3",
    "prisma": "^6.6.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@subsquid/evm-typegen": "^4.3.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@types/node": "^18.0.0",
    "eslint": "^9.25.1",
    "pino-pretty": "^13.0.0",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@10.6.1"
}
